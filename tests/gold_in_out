    "format": "morphology", 
    "query": [
        {
            "action": {
                "method": {
                    "query": {
                        "name": ["LAM", "lam"]
                    }
                }
            }, 
            "object": {
                "class": "Neuropil"
            }
        }, 
        {
            "action": {
                "method": {
                    "traverse_owns": {
                        "cls": "Neuron"
                    }
                }
            }, 
            "object": {
                "memory": 0
            }
        }
    ], 
    "user": "test"
}
